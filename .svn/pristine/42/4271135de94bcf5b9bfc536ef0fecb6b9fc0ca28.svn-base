  }, {\n            name: 'Bakso Mangkok',\n            price_sell: 25000\n        }, {\n            name: 'Bakso Pedas',\n            price_sell: 22000\n        }, {\n            name: 'Bakso Selimut Tetangga',\n            price_sell: 25000\n        }, {\n            name: 'Bakso Telur',\n            price_sell: 22000\n        }, {\n            name: 'Bakso Urat',\n            price_sell: 22000\n        }, {\n            name: 'Es Jeruk',\n            price_sell: 11000\n        }];\n\n        this.products = this.productDummies;\n\n        this.sale = this.shareProvider.getSale();\n\n        if (typeof this.sale == 'undefined') {\n            this.sale = {\n                amount: 0\n            };\n        }\n    }\n\n    ionViewDidLoad() {\n        console.log('ionViewDidLoad ProductFoodPage');\n    }\n\n    cancel() {\n        this.sale = {\n            amount: 0\n        };\n\n        this.shareProvider.setSale(this.sale);\n    }\n\n    checkout() {\n        this.app.getRootNav().push('PaymentPage');\n    }\n\n    select(product) {\n        if (typeof product.quantity == 'undefined') {\n            product.quantity = 0;\n        }\n\n        this.sale.amount += parseFloat(product.price_sell);\n\n        this.shareProvider.setSale(this.sale);\n    }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/product-food/product-food.ts"],"sourceRoot":""}