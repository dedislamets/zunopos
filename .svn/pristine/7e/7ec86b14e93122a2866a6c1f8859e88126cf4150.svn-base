e&select[3]=editable&select[4]=active&select[5]=cogs&select[6]=created_by__name&select[7]=created_by_id&select[8]=name&select[9]=number&select[10]=number2&select[11]=price_buy&select[12]=price_sell&select[13]=updated_by__name&select[14]=updated_by_id&select[15]=id&select[16]=unit_name&select[17]=unit_name2&select[18]=unit_name3&select[19]=unit_name4&select[20]=unit_name5&select[21]=unit_conversion&select[22]=unit_conversion2&select[23]=unit_conversion3&select[24]=unit_conversion4&select[25]=unit_conversion5&select[26]=max_quantity&select[27]=min_quantity&select[28]=max_quantity_unit_conversion&select[29]=min_quantity_unit_conversion&select[30]=active&filter[0][operator]=eq&filter[0][value]=1&filter[0][field]=category_id&filter[0][mode]=and&sorter[0][direction]=ASC&sorter[0][field]=name&limit=25&offset=0', param).subscribe(postData => {\n            this.products = postData['products'];\n\n            for (let product of this.products) {\n                product.price_sell = Math.round(parseFloat(product.price_sell) * 1000 / 1000);\n            }\n        });*/\n\n        this.productDummies = [{\n            name: 'Air Mineral',\n            price_sell: 5000\n        }, {\n            name: 'Bakso Ayam',\n            price_sell: 18000\n        }, {\n            name: 'Bakso Bom Beranak',\n            price_sell: 30000\n        }, {\n            name: 'Bakso Geprek',\n            price_sell: 15000\n        }, {\n            name: 'Gakso Granat',\n            price_sell: 18000\n        }, {\n            name: 'Bakso Iga',\n            price_sell: 35000\n        }, {\n            name: 'Bakso Keju',\n            price_sell: 22000\n        }, {\n            name: 'Bakso Mangkok',\n            price_sell: 25000\n        }, {\n            name: 'Bakso Pedas',\n            price_sell: 22000\n        }, {\n            name: 'Bakso Selimut Tetangga',\n            price_sell: 25000\n        }, {\n            name: 'Bakso Telur',\n            price_sell: 22000\n        }, {\n            name: 'Bakso Urat',\n            price_sell: 22000\n        }, {\n            name: 'Es Jeruk',\n            price_sell: 11000\n        }, {\n            name: 'Es Teh Manis',\n            price_sell: 5000\n        }, {\n            name: 'Es Teh Tawar',\n            price_sell: 3000\n        }, {\n            name: 'MociMoci Bubble Gum',\n            price_sell: 12000\n        }];\n\n        this.products = this.productDummies;\n\n        this.sale = this.shareProvider.getSale();\n\n        if (typeof this.sale == 'undefined') {\n            this.sale = {\n                amount: 0\n            };\n        }\n    }\n\n    ionViewDidLoad() {\n        console.log('ionViewDidLoad ProductAllPage');\n    }\n\n    cancel() {\n        this.sale = {\n            amount: 0\n        };\n\n        this.shareProvider.setSale(this.sale);\n    }\n\n    checkout() {\n        this.app.getRootNav().push('PaymentPage');\n    }\n\n    select(product) {\n        if (typeof product.quantity == 'undefined') {\n            product.quantity = 0;\n        }\n\n        this.sale.amount += parseFloat(product.price_sell);\n\n        this.shareProvider.setSale(this.sale);\n    }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/product-all/product-all.ts"],"sourceRoot":""}