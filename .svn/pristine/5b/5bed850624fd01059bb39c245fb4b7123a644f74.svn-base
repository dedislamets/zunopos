 { IonicPageModule } from 'ionic-angular';\nimport { ProductDrinkPage } from './product-drink';\n\n@NgModule({\n  declarations: [\n    ProductDrinkPage,\n  ],\n  imports: [\n    IonicPageModule.forChild(ProductDrinkPage),\n  ],\n})\nexport class ProductDrinkPageModule {}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/product-drink/product-drink.module.ts","import { HttpClient } from '@angular/common/http';\nimport { Component } from '@angular/core';\nimport { AlertController, App, IonicPage, NavController, NavParams } from 'ionic-angular';\n\nimport { ShareProvider } from '../../providers/share/share';\n\n@IonicPage()\n@Component({\n    selector: 'page-product-drink',\n    templateUrl: 'product-drink.html',\n})\nexport class ProductDrinkPage {\n    private productDummies: any;\n    private products: any;\n    private sale: any;\n\n    constructor(public navCtrl: NavController, public navParams: NavParams,\n        private alertCtrl: AlertController,\n        private app: App,\n        private httpClient: HttpClient,\n        private shareProvider: ShareProvider) {\n\n        this.productDummies = [{\n            name: 'Air Mineral',\n            price_sell: 5000\n        }, {\n            name: 'Es Jeruk',\n            price_sell: 11000\n        }, {\n            name: 'Es Teh Manis',\n            price_sell: 5000\n        }, {\n            name: 'Es Teh Tawar',\n            price_sell: 3000\n        }];\n\n        this.products = this.productDummies;\n\n        this.sale = this.shareProvider.getSale();\n\n        if (typeof this.sale == 'undefined') {\n            this.sale = {\n                amount: 0\n            };\n        }\n    }\n\n    ionViewDidLoad() {\n      console.log('ionViewDidLoad ProductDrinkPage');\n    }\n\n    cancel() {\n        this.sale = {\n            amount: 0\n        };\n\n        this.shareProvider.setSale(this.sale);\n    }\n\n    checkout() {\n        this.app.getRootNav().push('PaymentPage');\n    }\n\n    select(product) {\n        if (typeof product.quantity == 'undefined') {\n            product.quantity = 0;\n        }\n\n        this.sale.amount += parseFloat(product.price_sell);\n\n        this.shareProvider.setSale(this.sale);\n    }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/product-drink/product-drink.ts"],"sourceRoot":""}