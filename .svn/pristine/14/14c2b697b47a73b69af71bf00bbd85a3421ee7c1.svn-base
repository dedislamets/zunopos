<ion-header>
  <ion-navbar color="zuno">
    <ion-buttons start>
        <button ion-button menuToggle>
          <ion-icon name="menu"></ion-icon>
        </button>
    </ion-buttons>      
    <ion-title>Point Of Sale</ion-title>
    <ion-buttons end>
        <button ion-button (click)="basketModal()">
          <ion-label></ion-label>
          <ion-icon name="basket"></ion-icon>
      </button>
    </ion-buttons>
  </ion-navbar>
</ion-header>

<ion-tabs tabsHighlight="true" tabsPlacement="top">
  <ion-tab [root]="tab1Root" tabTitle="Semua"></ion-tab>
  <ion-tab [root]="tab2Root" tabTitle="Makanan" tabIcon="pizza"></ion-tab>
  <ion-tab [root]="tab3Root" tabTitle="Minuman" tabIcon="beer"></ion-tab>
  <ion-tab [root]="tab4Root" tabTitle="Lain-Lain" tabIcon="restaurant"></ion-tab>
</ion-tabs>
